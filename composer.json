{
  "require": {
    "craftcms/ckeditor": "4.6.0",
    "craftcms/cms": "5.6.7",
    "craftcms/store-hours": " 4.2.0",
    "ether/seo": "5.0.0",
    "ether/simplemap": "5.0.4",
    "mmikkel/cp-field-inspect": "2.0.4",
    "nystudio107/craft-cookies": "5.0.0",
    "nystudio107/craft-minify": "5.0.0",
    "nystudio107/craft-vite": "5.0.1",
    "rias/craft-position-fieldtype": "5.0.0",
    "topshelfcraft/supersort": "5.0.0",
    "topshelfcraft/wordsmith": "5.0.0",
    "verbb/super-table": "4.0.3",
    "vlucas/phpdotenv": "^5.4.0",
    "xpertbot/craft-wheelform": "4.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}