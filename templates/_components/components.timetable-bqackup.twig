      </li>
    </ul>
  </div>

  <div class="events">
    <ul>
      {% set programElements = craft.entries.section('mutationProgramDetails').all() %}
      {% set timetableLabels = mutationGlobals.timetableLocations %}
      {% set columns = timetableLabels|first %}

      {% if timetableLabels is defined and (timetableLabels|length) >= 2 %}
        {% set firstRow = timetableLabels[0] %}
        {% set secondRow = timetableLabels[1] %}

        {% for key, value in firstRow %}
          {% if secondRow[key] is defined and value != 'Location' and (key|slice(0, 3)) != 'col' and value != '' %}
            <li class="events-group">
              <div class="top-info">
                <span class="c-top-info__title">{{ value }}</span>
                {% if secondRow[key]|length %}
                  <span class="c-top-info__info">{{ secondRow[key] }}</span>
                {% endif %}
              </div>
              <ul class="js-sort">
                {% for item in programElements %}
                  {% if item.mutationRoom == key and item.programDate == day %}
                    {% include '_components/components.timetable-entry.twig' %}
                  {% endif %}
                {% endfor %}
              </ul>
            </li>
          {% endif %}
        {% endfor %}
      {% endif %}
      <!-- Kopergieterij -->
    </ul>
  </div>
</div>
